{
  "extends": [
    "tslint-config-standard"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "no-empty-interface": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-unnecessary-type-assertion": false,
    "no-console": false
  }
}
