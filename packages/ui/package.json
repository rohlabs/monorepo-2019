  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@emotion/babel-preset-css-prop": "^10.0.5",
    "@types/rebass": "^0.3.3",
    "@types/styled-components": "^4.1.4",
    "docz-theme-default": "^0.13.7",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "rebass": "^3.0.0",
    "tscpaths": "^0.0.6",
    "typescript": "^3.2.2"
  }
