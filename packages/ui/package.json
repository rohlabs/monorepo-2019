    "mapalias": "tscpaths -p tsconfig.json -s ./src -o ./dist/src",
    "build": "docz build"
  },
  "dependencies": {
    "docz": "^0.13.7",
    "docz-core": "^0.13.7",
    "react": "^16.7.0",
